<Page x:Class="AVTOTEST.Pages.ExaminationResultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AVTOTEST.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="650" d:DesignWidth="500"
      Title="ExaminationResultPage" Background="Gray">

    <Grid>
       <Label  Content="Result" Height="50" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="35"></Label>
        <StackPanel Margin="50">
            <Label FontSize="25">Correct Answer</Label>
            <TextBlock Name="CorrectAnswerCount" FontSize="25" Text="0"></TextBlock>
            <Label FontSize="25">QeuestionCount</Label>
            <TextBlock Name="QeustionCount" FontSize="25" Text="0"></TextBlock>
        </StackPanel>
        <Button Click="MenuButtonClick" Width="100" Height="40" VerticalAlignment="Bottom" HorizontalAlignment="Right" Content="Menu"></Button>
    </Grid>                             
</Page>                                 
